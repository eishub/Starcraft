use ProtossNexus as knowledge.
use ProtossNexus as actionspec.

module ProtossNexus {
	if a-goal(train("Protoss Probe")), bel(unit("Protoss Probe", X), X < 15) then train("Protoss Probe") + drop(train("Protoss Probe")).
	
	%if a-goal(trainMore("Protoss Probe")) then train("Protoss Probe") + drop(train("Protoss Probe")).
}