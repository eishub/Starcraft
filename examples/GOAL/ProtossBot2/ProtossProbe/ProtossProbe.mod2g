use ProtossProbe as knowledge.
use ProtossProbe as actionspec.

module ProtossProbe {
	if a-goal(build("Protoss Gateway", X, Y)) then build("Protoss Gateway", X, Y).
	
	if a-goal(build("Protoss Cybernetics Core", X, Y)) then build("Protoss Cybernetics Core", X, Y).
	
	if a-goal(build("Protoss Assimilator", X, Y)) then build("Protoss Assimilator", X, Y).
	
	if a-goal(gather(Id)), bel(not(busy)) then gather(Id).
}