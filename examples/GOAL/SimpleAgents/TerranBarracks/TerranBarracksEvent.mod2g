use TerranBarracks as knowledge.

module TerranBarracksEvent {
	if bel(percept(queueSize(N)), N < 2, percept(supply(Y,Z)), percept(minerals(X)), X>=50 , (Z - Y) >= 2 ) then{
			 if bel(not(trainMarine)) then insert(trainMarine).
			 if bel(not(trainMedic), percept(gas(G)), G >= 25, 
			 (percept(unit("Terran Marine", A)), percept(unit("Terran Medic", B)), A > (B * 2)); not(percept(unit("Terran Medic", _)))) 
			 then insert(trainMedic).
			 if bel(not(trainGhost), percept(gas(G)), G >= 75, percept(friendly(_, "Terran Covert Ops", _, _, _, _, _, _, _))) then insert(trainGhost).
		}
}