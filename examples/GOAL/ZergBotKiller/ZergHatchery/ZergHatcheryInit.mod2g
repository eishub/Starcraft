use ZergHatchery as knowledge.

module ZergHatcheryInit {
	if percept(enemyRace(Race)) then (overMind).send:(enemy(Race)) + insert(enemyRace(Race)).
	if percept(self(Id,Type)) then insert(self(Id, Type)).
	if percept(queueSize(Amount)) then insert(queueSize(Amount)).
	if percept(resources(M, G, C, T)) then insert(minerals(M), gas(G), supply(C, T)).
}