environment {
	env = "../../../eisbw/target/eisbw-0.0.1-SNAPSHOT-jar-with-dependencies.jar".
}

agentfiles{
	"ProtossNexus.goal" [name = protossNexus].
	"ProtossProbe.goal" [name = protossProbe].
}

launchpolicy{
	when [type = protossNexus]@env do launch protossNexus:protossNexus.
	when [type = protossProbe]@env do launch protossProbe:protossProbe.
}